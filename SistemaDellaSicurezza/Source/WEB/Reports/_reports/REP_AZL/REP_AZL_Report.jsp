	%>
		<html><body>
			<h2><%=c.printErrors()%></h2>
		</body></html>
	<%
	return;
}
out.clearBuffer();
Report report = ApplicationConfigurator.isModuleEnabled(MODULES.MOD_DVR_GSE)
        ? new Report_REP_AZL_GSE(lCOD_AZL)
        : new Report_REP_AZL(lCOD_AZL);
report.doReport(request, response);
%>
